{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AzureDefaults": {
    "Diagnostics": {
      "IsTelemetryDisabled": false,
      "IsLoggingContentEnabled": true
    },
    "Retry": {
      "MaxRetries": 3,
      "Mode": "Exponential"
    }
  },
  "Storage": {
    "Tables": "https://{account_name}.table.core.windows.net/"
  },
  "Arm": {
    "Subscription": "{subcriptionId}",
    "ResourceGroup": "{resourceGroup}"
  }
}
